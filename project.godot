; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PoReflexGame"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.4", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/GameManager.cs"
AndroidGameManager="*res://scripts/AndroidGameManager.cs"
LeaderboardManager="*res://scenes/game/LeaderboardManager.cs"

[display]

window/size/viewport_width=336
window/size/viewport_height=748
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[dotnet]

project/assembly_name="PoReflexGame"

[gui]

theme/custom="res://theme/game_theme.tres"

[rendering]

renderer/rendering_method="mobile"
renderer/rendering_method.mobile="Vulkan (Mobile)"
textures/vram_compression/import_etc2_astc=true
